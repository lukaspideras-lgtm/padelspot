{
  "expo": {
    "name": "padelspot",
    "slug": "padelspot",
    "version": "1.0.0",
    "runtimeVersion": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "padelspot",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.READ_CALENDAR",
        "android.permission.WRITE_CALENDAR",
        "android.permission.READ_CALENDAR",
        "android.permission.WRITE_CALENDAR"
      ],
      "package": "com.lukaspideras.padelspot"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-calendar",
        {
          "calendarPermission": "Padel Spot želi pristup vašem kalendaru kako bi automatski dodavao rezervacije."
        }
      ],
      ["expo-build-properties",{"android":{"useLegacyPackaging":true,"buildArchs":["arm64-v8a"],"enableMinifyInReleaseBuilds":true,"enableShrinkResourcesInReleaseBuilds":true}}]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "bc9dab83-c03c-43a9-962d-cf0c6eb936e7"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/bc9dab83-c03c-43a9-962d-cf0c6eb936e7"
    }
  }
}
